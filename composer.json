{
    "name": "hassankhan/frontman",
    "description": "A simple package to create your own Laravel-style facades",
    "keywords": [
        "hassankhan",
        "frontman",
        "facades"
    ],
    "homepage": "https://github.com/hassankhan/frontman",
    "license": "MIT",
    "authors": [
        {
            "name": "Hassan Khan",
            "email": "contact@hassankhan.me",
            "homepage": "http://hassankhan.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.2"
    },
    "autoload": {
        "psr-4": {
            "Frontman\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Frontman\\Tests\\": "tests"
        }
    }
}
